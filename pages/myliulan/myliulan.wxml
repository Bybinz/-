<view class="cu-list menu-avatar margintop2" wx:if="{{!noliulan}}">
  <view class="cu-item {{item.isleft?'move-cur':''}} heighter" wx:key bindtouchstart="ListTouchStart" bindtouchend="ListTouchEnd"
    data-itemid="{{item.id}}"  wx:for="{{liulanlist}}" bindtap="toarticledetail" data-articleid="{{item.articleId}}">
    <image class="cu-avatar  lg"
      src="{{item.firstimg}}" mode="aspectFit"></image>
    <view class="content">
      <view class="text-black ccsl">{{item.title}}</view>
      <view class="text-black ccsl">{{item.article}}</view>
    </view>
    <view class="move bg-red" catchtap="deleteitem" data-itemid="{{item.id}}"> 
      删除
    </view>
  </view>
</view>
<view wx:else>还没有浏览喔</view>